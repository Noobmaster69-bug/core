{
  "name": "core",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": "./index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon index.js",
    "setup_armhf": "export AR=\"arm-linux-gnueabihf-ar\" && export CC=\"arm-linux-gnueabihf-gcc\" && export CXX=\"arm-linux-gnueabihf-g++\" &&export LINK=\"arm-linux-gnueabihf-g++\"&&npm install --build-from-source --target_arch=arm --omit=dev",
    "setup": "npm install --omit=dev",
    "compile": "pkg . --no-bytecode --public-packages \"*\" --public",
    "build": "DOCKER_BUILDKIT=1 docker build --no-cache --output dist .",
    "clean": "rm -r ./dist | rm -r ./node_modules"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "express": "^4.18.1",
    "matcher": "^4.0.0",
    "morgan": "^1.10.0",
    "sequelize": "^6.17.0",
    "socket.io": "^4.5.1",
    "sqlite3": "^5.0.2"
  },
  "pkg": {
    "script": "*.js",
    "assets": "node_modules/sqlite3/lib/binding/**/*.node",
    "targets": [
      "node16-linux-armv7"
    ],
    "outputPath": "dist"
  }
}
